<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hosinsa.mapper.CrawlingMapper">
	<!-- crawling -->
	<update id="crwaling" parameterType="java.util.List">
		<foreach collection="list" item="product" open="INSERT ALL" close="SELECT * FROM DUAL" separator=" ">
			into HOSINSA_PRODUCT values (#{product.category}, #{product.proname}, #{product.proimg}, #{product.brand}, #{product.pronum}, #{product.price},
										#{product.stock}, #{product.proview}, #{product.detail})
        </foreach>
	</update>
